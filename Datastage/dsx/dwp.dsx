FUNCTION RunJobAsBackgroundWithInvocationLimit(JobName,InvocationID,InvocationLimit,SleepTime)
**************************************************************************************************
* MessageReporterStarter.                                                                        *
**************************************************************************************************
* Naming convention                                                                              *
* jN$ - Job name                                                                                 *
* jH$ - Job handler                                                                              *
* jIi$ - Job invokation id                                                                       *
* jS$ - Job status                                                                               *
* rN$ - Routine name                                                                             *
* st$ - stage                                                                                    *
* ln$ - link                                                                                     *
* i$ - integer variable                                                                          *
* c$ - character variable                                                                        *
* b$ - boolean variable                                                                          *
* i$list$ - integer variables array                                                              *
* c$list$ - character variables array                                                            *
* i$ar$ - integer variables dynamic array (@FM, @VM, @SV separated)                              *
* c$ar$ - character variables dynamic array (@FM, @VM, @SV separated)                            *
* L$ - Label                                                                                     *
* f$ - file handler                                                                              *
**************************************************************************************************
* .cjt - Job Controller temporary files extension                                                *
**************************************************************************************************

$include DSINCLUDE JOBCONTROL.H

      rN$MyName = \"RunJobAsBackgroundWithInvocationLimit\"

      i$ErrorCode = 0
      c$ErrorMsg = \"\"
      c$list$Parameters = \"\"
            
      jN$Target = JobName      

      jH$TargetRun = DSAttachJob(jN$Target:\".\":InvocationID, DSJ.ERRNONE)

      i$Status = DSGetJobInfo(jH$TargetRun, DSJ.JOBSTATUS)

      IF i$Status = DSJS.RUNNING 
      THEN 
         
         GoTo L$EXIT 
      END
      

*****************************************************************
      l$CHECKINVCOUNT:

      jH$Target = DSAttachJob(jN$Target, DSJ.ERRNONE)

      c$list$Invocations = DSGetJobInfo(jH$Target, DSJ.JOBINVOCATIONS)

      i$InvocationCount = DCOUNT(c$list$Invocations, \",\")

      i$RunCount = 0           
      i$InvIterator = 1           
 
      LOOP

      WHILE i$InvIterator <= i$InvocationCount DO
         c$Invocation = field(c$list$Invocations, \",\", i$InvIterator, 1)
         
         jH$InvocationTarget = DSAttachJob(c$Invocation, DSJ.ERRNONE)
         If Not(jH$InvocationTarget) THEN goto l$ATTACHERROR

         i$Status = DSGetJobInfo(jH$InvocationTarget, DSJ.JOBSTATUS)

         IF i$Status = DSJS.RUNNING THEN i$RunCount = i$RunCount + 1
         
         i$TargetDetachErrCode = DSDetachJob(jH$InvocationTarget)
         
         If i$TargetDetachErrCode <> DSJE.NOERROR THEN goto l$DETACHERROR

         i$InvIterator = i$InvIterator + 1
      REPEAT
 
      IF i$RunCount >= InvocationLimit THEN      
         sleep SleepTime
         goto l$CHECKINVCOUNT
      END


      c$list$Parameters = DSGetJobInfo(DSJ.ME, DSJ.PARAMLIST)

      Call DSLogInfo(c$list$Parameters, rN$MyName)

      i$ParamsCnt = DCOUNT(c$list$Parameters, \",\")

      jH$Target = DSAttachJob(jN$Target:\".\":InvocationID, DSJ.ERRNONE)

      If Not(jH$Target)
      Then
      l$ATTACHERROR:
         c$ErrorMsg = \"\(41E)\(448)\(438)\(431)\(43A)\(430) \(432)\(44B)\(437)\(43E)\(432)\(430) DSAttachJob. \(41D)\(435)\(432)\(43E)\(437)\(43C)\(43E)\(436)\(43D)\(43E) \(43F)\(43E)\(434)\(43A)\(43B)\(44E)\(447)\(438)\(442)\(44C)\(441)\(44F) \(43A) \(437)\(430)\(434)\(430)\(43D)\(438)\(44E) \":jN$Target
         GoTo L$ERROREXIT
      End

      jH$Target = DSPrepareJob(jH$Target)

      If Not(jH$Target)
      Then
         c$ErrorMsg = \"\(41E)\(448)\(438)\(431)\(43A)\(430) \(432)\(44B)\(437)\(43E)\(432)\(430) DSPrepareJob. \(41D)\(435)\(432)\(43E)\(437)\(43C)\(43E)\(436)\(43D)\(43E) \(43F)\(43E)\(434)\(433)\(43E)\(442)\(43E)\(432)\(438)\(442)\(44C) \(43A) \(437)\(430)\(43F)\(443)\(441)\(43A)\(443) \(437)\(430)\(434)\(430)\(43D)\(438)\(435) \":jN$Target
         GoTo L$ERROREXIT
      End

      i$Iteration = 0

***************************
* Set Parameters in Cycle *
***************************

      LOOP

         i$Iteration = i$Iteration + 1

      WHILE i$Iteration <= i$ParamsCnt DO

      c$ParamName = field(c$list$Parameters, \",\", i$Iteration, 1)

      Call DSLogInfo(c$ParamName, rN$MyName)

      c$value = DSGetParamInfo (DSJ.ME, c$ParamName, DSJ.PARAMVALUE)

      i$SetParamError = DSSetParam (jH$Target, c$ParamName, c$value)
      If i$SetParamError <> DSJE.NOERROR
      Then
         CONTINUE
         **c$ErrorMsg = \"\(41E)\(448)\(438)\(431)\(43A)\(430) \(432)\(44B)\(437)\(43E)\(432)\(430) DSSetParam. \(41D)\(435)\(432)\(43E)\(437)\(43C)\(43E)\(436)\(43D)\(43E) \(43F)\(435)\(440)\(435)\(434)\(430)\(442)\(44C) \(43F)\(430)\(440)\(430)\(43C)\(435)\(442)\(440) \" : c$ParamName : \" \(437)\(430)\(434)\(430)\(43D)\(438)\(44E) \":jN$Target
         **GoTo L$ERROREXIT

      End

L$NEXT:

      REPEAT

*************
* Cycle End *
*************

      i$SetLimitError = DSSetJobLimit(jH$Target, DSJ.LIMITROWS, 0)
      If i$SetLimitError <> DSJE.NOERROR
      Then
         c$ErrorMsg = \"\(41E)\(448)\(438)\(431)\(43A)\(430) \(432)\(44B)\(437)\(43E)\(432)\(430) DSSetJobLimit. \(41D)\(435)\(432)\(43E)\(437)\(43C)\(43E)\(436)\(43D)\(43E) \(437)\(430)\(434)\(430)\(442)\(44C) \(437)\(43D)\(430)\(447)\(435)\(43D)\(438)\(435) LIMITROWS \(437)\(430)\(434)\(430)\(43D)\(438)\(44E) \":jN$Target
         GoTo L$ERROREXIT
      End

      i$SetLimitError = DSSetJobLimit(jH$Target, DSJ.LIMITWARN, 0)
      If i$SetLimitError <> DSJE.NOERROR
      Then
         c$ErrorMsg = \"\(41E)\(448)\(438)\(431)\(43A)\(430) \(432)\(44B)\(437)\(43E)\(432)\(430) DSSetJobLimit. \(41D)\(435)\(432)\(43E)\(437)\(43C)\(43E)\(436)\(43D)\(43E) \(437)\(430)\(434)\(430)\(442)\(44C) \(437)\(43D)\(430)\(447)\(435)\(43D)\(438)\(435) LIMITWARN \(437)\(430)\(434)\(430)\(43D)\(438)\(44E) \":jN$Target
         GoTo L$ERROREXIT
      End

      GoTo L$SKIPMDG

      *** Switch Off Metadata Generation ***
      i$SetLimitError = DSSetGenerateOpMetaData(jH$Target, @FALSE)

      If i$SetLimitError <> DSJE.NOERROR
      Then
         c$ErrorMsg = \"\(41E)\(448)\(438)\(431)\(43A)\(430) \(432)\(44B)\(437)\(43E)\(432)\(430) DSSetGenerateOpMetaData. \(41D)\(435)\(432)\(43E)\(437)\(43C)\(43E)\(436)\(43D)\(43E) \(43E)\(442)\(43A)\(43B)\(44E)\(447)\(438)\(442)\(44C) \(433)\(435)\(43D)\(435)\(440)\(430)\(446)\(438)\(44E) \(43E)\(43F)\(435)\(440)\(430)\(446)\(438)\(43E)\(43D)\(43D)\(44B)\(445) \(43C)\(435)\(442)\(430)\(434)\(430)\(43D)\(43D)\(44B)\(445) \(437)\(430)\(434)\(430)\(43D)\(438)\(44F) \":jN$Target
         GoTo L$ERROREXIT
      End

L$SKIPMDG:

      i$RunError = DSRunJob(jH$Target, DSJ.RUNNORMAL)
      If i$RunError <> DSJE.NOERROR
      Then
         c$ErrorMsg = \"\(41E)\(448)\(438)\(431)\(43A)\(430) \(437)\(430)\(43F)\(443)\(441)\(43A)\(430) \(437)\(430)\(434)\(430)\(43D)\(438)\(44F) \":jN$Target
         GoTo L$ERROREXIT
      End

      i$TargetDetachErrCode = DSDetachJob(jH$Target)

      If i$TargetDetachErrCode <> DSJE.NOERROR
      Then
         l$DETACHERROR: 
         c$ErrorMsg = \"\(41E)\(448)\(438)\(431)\(43A)\(430) \(432)\(44B)\(437)\(43E)\(432)\(430) DSDetachJob. \(41D)\(435)\(432)\(43E)\(437)\(43C)\(43E)\(436)\(43D)\(43E) \(43E)\(442)\(43F)\(443)\(441)\(442)\(438)\(442)\(44C) \(437)\(430)\(434)\(430)\(43D)\(438)\(435) \":jN$Target
         GoTo L$ERROREXIT
      End

****************************
* End Of The Routine Logic *
****************************

      GoTo L$EXIT

L$ERROREXIT:
      Call DSLogWarn(c$ErrorMsg, rN$MyName)
      i$ErrorCode = 1

L$EXIT:
      Ans = i$ErrorCode

RETURN(Ans)
